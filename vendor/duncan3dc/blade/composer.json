{
    "name": "duncan3dc/blade",
    "type": "library",
    "description": "Use Laravel Blade templates without the full Laravel framework",
    "keywords": ["laravel", "templating", "template", "views"],
    "homepage": "https://github.com/duncan3dc/blade",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Craig Duncan",
        "email": "git@duncanc.co.uk",
        "homepage": "https://github.com/duncan3dc",
        "role": "Developer"
    }],
    "require": {
        "illuminate/contracts": ">=5.1,<5.5",
        "illuminate/view": ">=5.1,<5.5",
        "illuminate/events": ">=5.1,<5.5",
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "duncan3dc/object-intruder": "^0.1.0",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "duncan3dc\\Laravel\\": "src/",
            "duncan3dc\\LaravelTests\\": "tests/"
        }
    }
}
